#include <stdio.h>

int main() {
    int lateDays;
    int fine = 0;

    // Input number of late days
    printf("Enter number of late days: ");
    scanf("%d", &lateDays);

    // Calculate fine
    if (lateDays <= 5) {
        fine = lateDays * 2;
        printf("Fine ₹%d\n", fine);
    } else if (lateDays <= 10) {
        fine = 5 * 2 + (lateDays - 5) * 4;
        printf("Fine ₹%d\n", fine);
