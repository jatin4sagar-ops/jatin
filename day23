#include <stdio.h>

int main() {
    int n;
    double sum = 0.0;
    int numerator = 2, denominator = 3;

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    for(int i = 1; i <= n; i++) {
        sum += (double)numerator / denominator;  
        numerator += 2;   
        denominator += 4; 
    }

    printf("Sum of the series = %.2lf\n", sum);

    return 0;
}


Q2

#include <stdio.h>

int main() {
    int i, j;

    for(i = 1; i <= 5; i++) {        
        for(j = 1; j <= 5; j++) {    
            printf("*");
        }
        printf("\n");                 
    }

    return 0;
}
