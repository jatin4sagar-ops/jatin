#include <stdio.h>

int main() {
    int n = 13;  

   
    int starRows[] = {1, 3, 4, 6, 7, 8, 10, 11, 12, 13};
    int starCount = sizeof(starRows) / sizeof(starRows[0]);

    for(int i = 1; i <= n; i++) {
        int printStar = 0;
        for(int j = 0; j < starCount; j++) {
            if(i == starRows[j]) {
                printStar = 1;
                break;
            }
        }
        if(printStar)
            printf("*\n");
        else
            printf("\n");  
    }

    return 0;
}
